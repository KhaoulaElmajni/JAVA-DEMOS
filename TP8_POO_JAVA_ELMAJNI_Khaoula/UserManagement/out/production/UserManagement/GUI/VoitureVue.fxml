<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.geometry.Insets?>

<BorderPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="VoitureController">
    <top>
        <HBox spacing="20" >
            <padding>
                <Insets topRightBottomLeft="5"></Insets>
            </padding>
            <Label text="Marque"></Label>
            <TextField fx:id="fieldMat"></TextField>
            <Label text="Matricule"></Label>
            <TextField fx:id="fieldMar"></TextField>
            <Button text="add" onAction="#ajouterVoiture"></Button>
            <Button text="delete" onAction="#supprimerVoiture"></Button>
        </HBox>
    </top>
    <center>
        <ListView fx:id="listview">

        </ListView>
    </center>

</BorderPane>
